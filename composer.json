{
    "name": "sergeil/doctrine-entity-data-mapper-bundle",
    "description": "Makes it easy to map data coming from client-side onto your Doctrine ORM managed entities.",
    "keywords": ["doctrine"],
    "homepage": "http://github.com/sergeil/SliDoctrineEntityDataMapperBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Sergei Lissovski",
            "email": "sergei.lissovski@gmail.com",
            "homepage": "http://sergei.lissovski.org/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/symfony": "~2.0|~3.0",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "sergeil/aux-bundle": "~1.1",
        "sergeil/expander-bundle": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.0",
        "modera/foundation-bundle": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Sli\\DoctrineEntityDataMapperBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
